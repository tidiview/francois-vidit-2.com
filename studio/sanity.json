{
  "root": true,
  "api": {
    "projectId": "nlozcoql",
    "dataset": "staging"
  },
  "project": {
    "name": "francois-vidit-2.com"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "dark-theme-blue",
    "content-model-graph",
    "light-theme",
    "dark-mode"
  ],
  "env": {
    "development": {
      "plugins": [
        "@sanity/vision"
      ]
    },
    "api": {
      "dataset": "staging"
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    }
  ],
  "__experimental_spaces": [
    {
      "name": "production",
      "title": "Production",
      "default": false,
      "api": {
        "projectId": "nlozcoql",
        "dataset": "production"
      }
    },
    {
      "name": "staging",
      "title": "Staging",
      "default": true,
      "api": {
        "projectId": "nlozcoql",
        "dataset": "staging"
      }
    }
  ]
}
